<script>
    function hasReachedBagLimit(productcode, size, qtyToAdd, hideWishList){
        var limit = window.rcProps.maxLimit;
        var showLimitPopup = (parseInt($('#shopping_bag_count').html()) + parseInt(qtyToAdd)) > limit;
        if(showLimitPopup){
            if (window.rcProps.showReCAPTCHA){
                var data = $.param({
                    mobile: true
                });
                $.modal({
                    url: "/fw/dialog/VerifyHuman.jsp?" + data,
                    open: null,
                    cache: false,
                    overlayClose: true,
                    closeButton: false,
                    triggerOpen: true,
                    onClose : function(){
                        if(window.rcProps && typeof window.rcProps.ReCAPTCHARestore =='function'){
                            window.rcProps.ReCAPTCHARestore();
                            window.rcProps.ReCAPTCHARestore = "";
                        }
                    }
                });
            } else {
                var limitData = $.param({
                    code: productcode,
                    size: size,
                    mobile: true,
                    hideWishList: hideWishList
                });
                $.modal({
                    url: "/fw/dialog/ShoppingBagLimit.jsp?" + limitData,
                    type: "small",
                    open: null,
                    cache: false,
                    overlayClose: true,
                    closeButton: false,
                    triggerOpen: true,
                    onComplete: function () {
                    }
                });
            }
        }
        return showLimitPopup;
    }

